@model Restaurante.Models.Reserva

@{
    ViewData["Title"] = "Details";
}
<hr/>
<h3>Detalles</h3>
<h3>Reserva</h3>
<div>    
    <hr />
    <dl class="row">
        <dt class = "col-sm-2"> 
            Fecha
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Fecha)
        </dd>       
        <dt class = "col-sm-2">
            Hora
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Hora)
        </dd>
        <dt class = "col-sm-2">
            Ci Cliente
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CiClienteNavigation.Ci)
        </dd>
        <dt class = "col-sm-2">
            Id Mesa
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdMesaNavigation.Id)
        </dd>
        <dt class="col-sm-2">
            Estado
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Estado)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Editar</a>    
</div>
<hr />
<div>
    <a class="btn btn-primary" href="@Url.Action("Index", "Reserva")">Volver</a>
</div>
